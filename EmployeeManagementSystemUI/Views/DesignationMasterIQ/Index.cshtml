@model IEnumerable<EmployeeManagementSystemUI.Models.DesignationMaster2>

@{
    ViewData["Title"] = "Designations";
}

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>Designation Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.DesignationName</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.DesignationId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.DesignationId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.DesignationId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>